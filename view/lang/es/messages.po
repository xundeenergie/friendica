msgid ""
"Default system theme - may be over-ridden by user profiles - <a href=\"%s\" "
"id=\"cnftheme\">Change default theme settings</a>"
msgstr ""

#: src/Module/Admin/Site.php:480
msgid "Mobile system theme"
msgstr "Tema de sistema móvil"

#: src/Module/Admin/Site.php:480
